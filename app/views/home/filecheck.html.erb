<%= form_for @upload, url: home_filecreate_path, html: { multipart: true } do |f| %>

      <%= f.label "가격" %>
      <%= @count.to_i*50 %> 원
      <br>

      <%= f.label "파일명" %>
      <%= params[:upload][:attachment].original_filename %>

      <br>

      <%= f.label "복사대" %>
      <%= params[:upload][:printer]%>
      <br>

      <%= f.label "픽업날짜" %>
      <%= params[:upload][:pkupdate]%>
      <br>

      <%= f.label "픽업시간" %>
      <%= params[:upload][:pkuptime]%>
      <br>

      <%= f.label "인쇄정보" %>
      <%= params[:upload][:landscape] + "/" +params[:upload][:doublepg] + "/" + params[:upload][:split] + "/" +
      params[:upload][:color]%>
      <br>

      <h2>신청한 내용을 확인하시겠습니까?</h2>


      <%= f.submit "결제하기", class: "btn btn-primary" %>
   <% end %>